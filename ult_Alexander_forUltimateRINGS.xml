    <t no="1" name="InsertDiceMarker" sync="^1B:[id8]:[pc]:0000:[id4]:(?&lt;mark_code&gt;[0-9a-fA-F]{4}):">
      <expressions>
        <table>
        {
            "method":"Insert", "table":"DiceTable",
            "cols" : [
              { "name":"marker", "val":"${mark_code}", "key":"true" },
              { "name":"target", "val":"${_pc}" },
            ]
        }
        </table>
      </expressions>
    </t>

    <t no="2" sync="^1B:" sync-count="8" text="１[北] ソード" notice="ひとばん、北、ソード">
      <expressions>
        <pre name="TABLE['DiceTable'][0]['target']" value="@Model.Player.Name" />
        <set name="dice_group" value="1" />
      </expressions>
      <v-notice icon="Dice1.png" duration="30" duration-visible="false" order="-2" sync-to-hide="ブルートジャスティス:コードネーム「ブルートジャスティス」" />
    </t>

    <t no="3" sync="^1B:" sync-count="8" text="２[北] チャージ" notice="ふたばん、北、チャージ">
