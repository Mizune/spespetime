        <set name="water_bomb" value="false" />
      </expressions>
    </t>

    <t name="雷圧縮解除" sync="[mex] loses the effect of 雷属性圧縮">
      <expressions>
        <set name="thunder_bomb" value="false" />
      </expressions>
