    <t text="α１" sync="[mex]に「最後の仮判決α」の効果。$" notice="あるふぁー1">
      <expressions>
        <pre name="judge_done" value="false" />
      </expressions>
    </t>

    <t text="β１" sync="[mex]に「最後の仮判決β」の効果。$" notice="べーたー1">
      <expressions>
        <pre name="judge_done" value="false" />
