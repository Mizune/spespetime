    <a time="@(145 + (2.2 * 8) - origin)" text="ブラスター九" notice-o="0" notice="9" />
    <a time="@(145 + (2.2 * 8) - origin)" text="ソード＆チャージ" notice-o="0.1" notice="前えー">
      <expressions>
        <pre name="dice_group" value="4" />
      </expressions>
      <v-notice
