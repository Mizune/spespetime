      <v-notice
        text="１, ２前に出る"
        icon="True.png"
        duration="1.5"
        duration-visible="false"
        order="-1" />
    </a>
    <a time="@(145 + (2.2 * 2) - origin)" text="ソード＆チャージ" notice-o="0.1" notice="サイドで回避">
      <expressions>
        <pre name="dice_group" value="3" />
      </expressions>
      <v-notice
        text="サイドで回避"
        icon="True.png"
        duration="1.5"
        duration-visible="false"
        order="-1" />
