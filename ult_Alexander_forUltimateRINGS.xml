        duration="29"
        icon="Lightning.png"
        sync-to-hide="[mex] loses the effect of 雷属性圧縮 from" />
    </t>

    <t name="水ダウン解除" sync="[mex] loses the effect of 水属性耐性低下［強］">
      <expressions>
        <set name="water_down" value="false" />
      </expressions>
    </t>

