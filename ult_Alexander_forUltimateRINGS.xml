    <t text="γ１" sync="[mex]に「最後の仮判決γ」の効果。$" notice="がんまー1">
      <expressions>
        <pre name="judge_done" value="false" />
      </expressions>
    </t>

    <t text="δ１" sync="[mex]に「最後の仮判決δ」の効果。$" notice="でるたー1">
